# Firebase Configuration
# Get these values from your Firebase project settings
# Go to Project Settings > General > Your apps > Web app > Config
VITE_FIREBASE_API_KEY=your-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Admin Credentials for Scripts
# Used by various population and admin scripts
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=your-admin-password

# Production Deployment Settings
# Used by Docker and deployment scripts
NODE_ENV=production

# YouTube Data API
VITE_YOUTUBE_API_KEY=your-youtube-api-key

# reCAPTCHA
VITE_RECAPTCHA_SITE_KEY=your-recaptcha-site-key

# Bumpups API
VITE_BUMPUPS_API_KEY=your-bumpups-api-key
VITE_BUMPUPS_PROXY_URL=https://your-firebase-functions-url/bumpupsProxy

# OpenAI API
VITE_OPENAI_API_KEY=your-openai-api-key
VITE_OPENAI_ASSISTANT_URL=https://us-central1-your-project.cloudfunctions.net

# Webshare Proxy API (REMOVED - no longer used)
# WEBSHARE_API_KEY=your-webshare-api-key
# WEBSHARE_API_URL=https://proxy.webshare.io/api/v2/proxy/list/

# Development Settings (optional)
# Uncomment for development customization
# VITE_APP_ENV=development
# VITE_DEBUG=true 