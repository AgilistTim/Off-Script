# Firebase Configuration
# Get these values from your Firebase project settings
# Go to Project Settings > General > Your apps > Web app > Config
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:abcdef1234567890
VITE_FIREBASE_MEASUREMENT_ID=G-ABCDEFGHIJ

# Admin Credentials for Scripts
# Used by various population and admin scripts
ADMIN_EMAIL=admin@offscript.com
ADMIN_PASSWORD=secure_admin_password_here

# Production Deployment Settings
# Used by Docker and deployment scripts
NODE_ENV=production

# YouTube Data API
VITE_YOUTUBE_API_KEY=your_youtube_api_key_here

# reCAPTCHA
VITE_RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here

# Bumpups API
VITE_BUMPUPS_API_KEY=your_bumpups_api_key_here

# Development Settings (optional)
# Uncomment for development customization
# VITE_APP_ENV=development
# VITE_DEBUG=true 